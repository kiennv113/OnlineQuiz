<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>NewFilter</filter-name>
        <filter-class>filter.NewFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>NewFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>makeQuizCon</servlet-name>
        <servlet-class>controller.makeQuizCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginCon</servlet-name>
        <servlet-class>controller.loginCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.registerCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>takeQuizCon</servlet-name>
        <servlet-class>controller.takeQuizCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>controller.NewServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>takeQuizAnser</servlet-name>
        <servlet-class>controller.takeQuizAnser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>manageQuizCon</servlet-name>
        <servlet-class>controller.manageQuizCon</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>makeQuizCon</servlet-name>
        <url-pattern>/makeQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginCon</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>takeQuizCon</servlet-name>
        <url-pattern>/takeQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/NewServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>takeQuizAnser</servlet-name>
        <url-pattern>/takeQuizAnser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manageQuizCon</servlet-name>
        <url-pattern>/manageQuiz</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
